{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://mus-project.onrender.com/api/:path*" },
    { "source": "/images/:path*", "destination": "https://mus-project.onrender.com/images/:path*" }
  ],
  "redirects": [
    { "source": "/", "destination": "/drug-recognition-demo/index.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}