{
  "version": 2,
  "name": "mus-project",
  "builds": [
    { "src": "drug-recognition-demo/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://YOUR-BACKEND-ON-RENDER.com/api/$1" },
    { "src": "/images/(.*)", "dest": "https://YOUR-BACKEND-ON-RENDER.com/images/$1" },
    { "src": "/(robots.txt|sitemap.xml)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/drug-recognition-demo/index.html" }
  ]
}